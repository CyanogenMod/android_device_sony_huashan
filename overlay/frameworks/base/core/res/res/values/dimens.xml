<!-- Change navigation bar height and width from the default 48x42dp to user-preferred 42x36dp -->
<resources>
    <!-- Height of the bottom navigation / system bar. -->
    <dimen name="navigation_bar_height">42dp</dimen>
    <!-- Height of the bottom navigation bar in portrait; often the same as @dimen/navigation_bar_height -->
    <dimen name="navigation_bar_height_landscape">42dp</dimen>
    <!-- Width of the navigation bar when it is placed vertically on the screen -->
    <dimen name="navigation_bar_width">36dp</dimen>
</resources>
